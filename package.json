{
  "name": "xmlhttprequest_polyfill",
  "version": "0.0.1",
  "description": "xmlhttprequest polyfill for the browser",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanfaucett/xmlhttprequest_polyfill.git"
  },
  "scripts": {
    "test": "npm run test-node",
    "test-node": "node ./test | ./node_modules/.bin/tap-spec",
    "test-node-raw": "node ./test",
    "test-browser-local": "zuul --local 8080 --ui tape -- ./test"
  },
  "main": "src/index.js",
  "keywords": [
