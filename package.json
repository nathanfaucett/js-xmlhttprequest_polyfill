  "readmeFilename": "README.md",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-jsbeautifier": "^0.2.7",
    "tap-spec": "^4.0.2",
    "tape": "^4.2.0",
    "zuul": "^3.2.0"
  },
  "dependencies": {
    "empty_function": "git://github.com/nathanfaucett/empty_function.git",
    "environment": "git://github.com/nathanfaucett/environment.git",
    "event_emitter": "git://github.com/nathanfaucett/event_emitter.git",
    "extend": "git://github.com/nathanfaucett/extend.git"
  }
